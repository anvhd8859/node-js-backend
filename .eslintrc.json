{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double", { "allowTemplateLiterals": true }],
        "max-len": "off",
        "import/extensions": "off",
        "no-return-await": "off",
        "import/prefer-default-export": "warn",
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    }
}
